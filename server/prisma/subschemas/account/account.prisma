model Account {
  id          Int           @id @default(autoincrement())
  uuid        String        @unique @default(uuid())
  user_id     Int
  name        String
  description String?
  currency    String
  created_at  DateTime      @default(now())
  updated_at  DateTime      @updatedAt
  User        User          @relation(fields: [user_id], references: [id])

  @@index([user_id])
}
